genrule(
    name = "copyfile",
    srcs = ["A.java"],
    cmd = "cp $SRCS $OUT",
    out = ".",
)

zip_file(
    name = "dep.src.zip",
    out = "dep.src.zip",
    srcs = [":copyfile",],
)

java_library(
    name = "zipdep",
    srcs = [
        "C.java",
        ":dep.src.zip",
    ],
    visibility = ["PUBLIC"],
)
