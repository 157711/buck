[java]
    # Indicates that any folder named src or test
    # are folders that contain Java code.
    src_roots = src, test
[alias]
    buck = //src/com/facebook/buck/cli:cli
[buildfile]
    includes = //DEFS
[project]
    ignore = .git, build
[test]
    # Maximum timeout of 1 minute per test.
    timeout = 60000
