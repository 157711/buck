    srcs = BUCK_PATHS_SRCS,
    autodeps = True,
    immutable_types = [
        "BuckPaths",
    ],
    visibility = ["PUBLIC"],
    deps = [
        "//src/com/facebook/buck/util/immutables:immutables",
        "//third-party/java/immutables:immutables",
    ],
)

